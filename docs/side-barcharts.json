{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/elii0017/FIT3179/refs/heads/main/Excel%20Files/Average-Daily-Imprisonment-Rate.csv",
      "format": {
        "type": "csv"
      }
    },
    "transform": [
      {
        "fold": ["Mar Qtr 2023", "Jun Qtr 2023", "Mar Qtr 2024", "Jun Qtr 2024"],
        "as": ["Quarter", "Value"]
      }
    ],
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "States",
        "type": "nominal",
        "title": "States",
        "axis": {"labelAngle": 45}
      },
      "y": {
        "field": "Value",
        "type": "quantitative",
        "title": "Value"
      },
      "color": {
        "field": "Quarter",
        "type": "nominal",
        "title": "Quarter"
      },
      "xOffset": {
        "field": "Quarter"
      }
    },
    "width": 800,
    "height": 300
  }
  
